package com.springdemos.SpringCore.propertyplaceholder;

public class MyDAO {

	String dbserver;
	String dburl;
	String dbpassword;
	@Override
	public String toString() {
		return "MyDAO [dbserver=" + dbserver + ", dburl=" + dburl + ", dbpassword=" + dbpassword + "]";
	}
	public MyDAO(String dbserver, String dburl, String dbpassword) {
		super();
		this.dbserver = dbserver;
		this.dburl = dburl;
		this.dbpassword = dbpassword;
	}
	
}
